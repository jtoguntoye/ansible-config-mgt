pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    sh 'echo "Building stage"'
                }
            }
        }
        stage('Test') {
            steps {
                script {
                    sh 'echo "Testing stage"'
                }
            }
        }
        stage('Package') {
            steps {
                script {
                    sh 'echo "Packaging stage"'
                }
            }
        }
        stage('Deploy') {
            steps {
                script {
                    sh 'echo "Deployment stage"'
                }
            }
        }
        stage('Clean up') {
            steps {
                script {
                    sh 'echo "clean up stage"'
                }
            }
        }
    }
}